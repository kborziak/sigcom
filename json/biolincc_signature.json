{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/signature.json",
    "${library.dataset}": "enrichr_geneset",
    "${library.meta.Library_name}": "^Intramyocardial.+$"
  },
  "properties": {
    "MASKID": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Original_String']}",
      "priority": 1,
      "subtitle": true
    },
    "TREATMENT_ASSIGNMENT": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Original_String']}",
      "name": true,
      "priority": 2
    }
  }
}